{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test-coverage": "craco test --coverage --watchAll=false",
    "serve-build": "serve ./build --single",
    "firebase-emulator": "firebase emulators:start --import=./local-firebase-data --export-on-exit",
    "firebase-emulator:test": "firebase emulators:start --import=./test-firebase-data",
    "cypress": "cypress open",
    "e2e": "cypress run --browser chrome",
    "lint": "eslint ./src/**/*.{tsx,ts}"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@loadable/component": "^5.14.1",
    "@mui/icons-material": "^5.10.2",
    "@mui/lab": "^5.0.0-alpha.138",
    "@mui/material": "^5.11.12",
    "@mui/x-data-grid": "^6.18.6",
    "@mui/x-date-pickers": "^7.6.1",
    "@react-pdf/renderer": "^2.3.0",
    "@reduxjs/toolkit": "^2.2.5",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^12.0.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "assert": "^2.0.0",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.4",
    "axios-mock-adapter": "^1.21.2",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "constate": "^3.2.0",
    "cron-parser": "^3.5.0",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "dayjs": "^1.11.11",
    "firebase": "^9.3.0",
    "firebase-tools": "^9.22.0",
    "graphql": "^15.4.0",
    "materialize-css": "1.0.0",
    "nanoid": "^3.1.28",
    "notistack": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.7.2",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^6.14.2",
    "react-intl": "^5.10.16",
    "react-materialize": "3.10.0",
    "react-query": "^3.39.3",
    "react-redux": "^9.1.2",
    "react-responsive-calendar": "^0.6.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^5.0.1",
    "react-tiny-fab": "^4.0.4",
    "react-toast-notifications": "^2.5.1",
    "stream-browserify": "^3.0.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.0.3",
    "util": "^0.12.4",
    "uuid": "^9.0.0",
    "web-vitals": "^0.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-restricted-imports": [
        "error",
        {
          "patterns": [
            "@mui/*/*/*"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@types/loadable__component": "^5.13.3",
    "@types/react-helmet": "^6.1.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-toast-notifications": "^2.4.0",
    "cypress": "^10.11.0",
    "cypress-firebase": "^2.0.0",
    "firebase-admin": "^10.0.2",
    "graphql-tag": "^2.11.0",
    "serve": "^12.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/api/**/*.{js,jsx,ts,tsx}",
      "!src/types/**/*.{js,jsx,ts,tsx}",
      "!src/intl/**/*.{js,jsx,ts,tsx}",
      "!src/App.tsx",
      "!src/Providers.tsx",
      "!src/Routing.tsx",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/MuiThemeProvider.tsx"
    ]
  },
  "overrides": {
    "react-redux": {
      "react": "$react",
      "@types/react": "$@types/react"
    },
    "react-toast-notifications": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@react-pdf/renderer": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-hook-form": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-materialize": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
